- content_for :title_block do
  = render 'site/partials/navigation'

.clearfix.align-group
  .span-8.align-element{data: {alignprimary: "true"}}
    #slideshow= render partial: 'site/partials/slideshow', locals: {articles: @model.slideshow, pages: []}

  .span-4.last.align-element{data: {aligntarget: '#editors-notes .rounded'}}
    = render partial: 'site/partials/advertisement', locals: {zone: :medium_rectangle}
    #editors-notes.title-medium= render partial: 'site/containers/article-list', locals: {title: "Editor's Notes", articles: @model.editors_notes, style: :featured}

#music= render partial: 'site/containers/article-row', locals: {title: 'Music', articles: @model.music, feature: true}
#film= render partial: 'site/containers/article-row', locals: {title: 'Film', articles: @model.film, feature: true}
#art= render partial: 'site/containers/article-row', locals: {title: 'Art', articles: @model.art, feature: true}
#multimedia= render partial: 'site/containers/multimedia-row', locals: {title: 'Multimedia', multimedia: @model.multimedia}
